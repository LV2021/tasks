#include <iostream>

int main()
{
	const int size = 5;
	int arr[size]{ 2,6,5,4 };
	int arr1[size]{ 5,9,8,7 };
	int arr3[size];
	for (int i = 0; i < size; ++i) {
		arr3[i] = arr[i] * arr1[i];
		std::cout << arr[i] << "*" << arr1[i] << "=" << arr[i] * arr1[i] << std::endl;
	}
	
	for (int i = 0; i < size; ++i) {
		std::cout << arr3[i] << " ";
	}
}
